{
  "name": "stunning-system-k11",
  "version": "1.0.0",
  "description": "Modern TypeScript/Node.js project with institutional-grade tooling and best practices",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist coverage",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:json",
    "lint:ts": "eslint src/**/*.ts --fix",
    "lint:md": "markdownlint '**/*.md' --fix",
    "lint:json": "jsonlint '**/*.json'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "api:generate": "redocly bundle api/openapi.yaml --output api/bundled.yaml",
    "api:validate": "redocly lint api/openapi.yaml",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "api",
    "swagger",
    "openapi",
    "institutional-grade",
    "modern",
    "best-practices"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "src",
    "api",
    "README.md",
    "LICENSE"
  ],
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint --fix"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@redocly/cli": "^2.0.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^16.1.5",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "packageManager": "npm@10.0.0"
}
